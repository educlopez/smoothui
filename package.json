    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "eslint": "^9.19.0",
    "eslint-config-next": "15.1.6",
    "postcss": "^8.5.1",
    "prettier": "^3.5.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "shiki": "^2.3.0",
    "tailwindcss": "^4.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
