{
  "name": "@educlopez/smoothui",
  "version": "1.1.0",
  "author": "@educalvolpz",
  "description": "SmoothUI is a collection of beautifully designed components with smooth animations built with React, Tailwind CSS, and Motion",
  "private": true,
  "license": "MIT",
  "keywords": [
    "components",
    "tailwindcss",
    "motion",
    "react"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "build:index": "ts-node src/scripts/build-registry-index.ts",
    "build:source": "ts-node src/scripts/build-registry-sources.ts",
    "build:registry": "pnpm run build:index && pnpm run build:source",
    "build": "pnpm run build:registry && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
